{
  "name": "use-konami",
  "version": "1.0.1",
  "description": "A zero dependency React hook that listens for a sequence of key presses to unlock a feature",
  "repository": "https://github.com/chris-feist/use-konami.git",
  "license": "MIT",
  "keywords": [
    "easter egg",
    "easteregg",
    "konami",
    "react hook",
    "secret",
    "cheatcode",
    "cheat code",
    "hook",
    "hooks"
  ],
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "esm/"
  ],
  "scripts": {
    "clean": "rimraf dist esm storybook-static",
    "prebuild": "yarn run clean",
    "build": "yarn build:cjs && yarn build:es && yarn build:storybook",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "test": "jest",
    "lint": "yarn run lint:eslint && yarn run lint:types",
    "lint:eslint": "eslint . --ext .ts,.tsx",
    "lint:types": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "publish:npm": "npm publish",
    "publish:storybook": "gh-pages -d storybook-static"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=16.13"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@testing-library/react-hooks": "^5.1.0",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.21.0",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "react-toastify": "^7.0.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  }
}
